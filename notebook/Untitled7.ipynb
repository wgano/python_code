    "import time\n",
    "b = time.time()\n",
    "\n",
    "max = input(\"输入:\")\n",
    "sum = 0\n",
    "for i in range(1,int(max)+1):\n",
    "    sub = 1\n",
    "    for j in range(1,i+1):\n",
    "        sub *= j\n",
    "        \n",
    "        \n",
    "    sum += sub\n",
    "print(sum)\n",
    "e = time.time()\n",
    "m, s = divmod(e-b, 60)\n",
    "h, m = divmod(m, 60)\n",
    "print(\"%d:%02d:%02d\" % (h, m, s))\n"
   ]
