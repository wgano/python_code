   "source": [
    "# 快慢指针找到链表中点\n",
    "fast = slow = head\n",
    "while fast.next and fast.next.next:\n",
    "    slow = slow.next\n",
    "    fast = fast.next.next\n",
    "    \n",
    "# 反转右半部分链表\n",
    "pre = None\n",
    "while cur:\n",
    "    t = cur.next\n",
    "    cur.next = pre\n",
    "    pre, cur = cur, t\n",
    "cur = head\n"
   ]
  }
 ],
